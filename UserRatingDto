package com.infodart.peerhuntr.dto.peerRating;

import java.util.Date;
import java.util.Map;

import com.infodart.peerhuntr.dto.user.UserDTO;

/**
 * DTO for the user_rating database table.
 * 
 */

public class UserRatingDTO {
	
	private int userRatingId;
	private Date createdOn;
	private Date modifiedOn;
	private String networkUser;
	private String remarks;
	private byte status;
	private RatingTypeDTO ratingType;
	private SocialNetworkMasterDTO socialNetworkMaster;
	private int userRatingValue;
	private String referralCode;
	private String network;
	private String emailId;
	private Map<String,Integer> ratings;
	

	
	
	public String getEmailId() {
		return emailId;
	}

	public void setEmailId(String emailId) {
		this.emailId = emailId;
	}

	public UserRatingDTO() {
	}

	public Map<String, Integer> getRatings() {
		return ratings;
	}

	public void setRatings(Map<String, Integer> ratings) {
		this.ratings = ratings;
	}

	public String getReferralCode() {
		return referralCode;
	}

	public void setReferralCode(String referralCode) {
		this.referralCode = referralCode;
	}

	

	public String getNetwork() {
		return network;
	}

	public void setNetwork(String network) {
		this.network = network;
	}

	public int getUserRatingId() {
		return this.userRatingId;
	}

	public void setUserRatingId(int userRatingId) {
		this.userRatingId = userRatingId;
	}

	public Date getCreatedOn() {
		return this.createdOn;
	}

	public void setCreatedOn(Date createdOn) {
		this.createdOn = createdOn;
	}

	public Date getModifiedOn() {
		return this.modifiedOn;
	}

	public void setModifiedOn(Date modifiedOn) {
		this.modifiedOn = modifiedOn;
	}

	public String getNetworkUser() {
		return this.networkUser;
	}

	public void setNetworkUser(String networkUser) {
		this.networkUser = networkUser;
	}

	public String getRemarks() {
		return this.remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}

	public byte getStatus() {
		return this.status;
	}

	public void setStatus(byte status) {
		this.status = status;
	}

	public RatingTypeDTO getRatingType() {
		return this.ratingType;
	}

	public void setRatingType(RatingTypeDTO ratingType) {
		this.ratingType = ratingType;
	}

	public SocialNetworkMasterDTO getSocialNetworkMaster() {
		return this.socialNetworkMaster;
	}

	public void setSocialNetworkMaster(SocialNetworkMasterDTO socialNetworkMaster) {
		this.socialNetworkMaster = socialNetworkMaster;
	}

	public int getUserRatingValue() {
		return userRatingValue;
	}

	public void setUserRatingValue(int userRatingValue) {
		this.userRatingValue = userRatingValue;
	}


}
